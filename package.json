{
  "name": "gd-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng_prod": "ng serve --port 80 --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start": "ng serve",
    "build": "ng build",
    "build_prod": "ng build --prod",
    "build_prod_aot": "ng build --prod --aot --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "node ./dist/prerender",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "npm run build_prod_aot && ng run gd-ui:server:production --bundleDependencies all",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server && npm run prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "angular-web-storage": "^7.0.0-beta.1",
    "chart.js": "^2.9.3",
    "express": "^4.15.2",
    "file-saver": "^2.0.2",
    "jquery": "^3.5.1",
    "net": "^1.0.2",
    "ng2-charts": "^2.2.4",
    "ngx-cookie-service": "^2.2.0",
    "ngx-image-cropper": "^3.0.3",
    "ngx-material-timepicker": "^5.2.3",
    "ngx-summernote": "^0.8.0",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0-rc.2",
    "request-promise": "^4.2.6",
    "rxjs": "~6.4.0",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "summernote": "^0.8.18",
    "summernote-file": "0.0.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/express": "^4.17.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  }
